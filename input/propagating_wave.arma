model = MA {
	out_grid = (100,200,100)
	acf = {
		func = cos
		amplitude = 2
		alpha = (6,1,0)
		velocity = 0.1
		beta = (0.25,0)
		nwaves = (1.5,4.5,1.5)
	}
	order = (32,1,1)
	algorithm = fixed_point_iteration
	output = surface,summary,acf
	validate = 1
}
velocity_potential_solver = high_amplitude {
#	wnmax = from (0,0) to (0,0.25) npoints (2,2)
	depth = 12
	domain = from (10,-12) to (10,3) npoints (1,128)
}
