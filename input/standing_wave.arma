model = AR {
	out_grid = (200,40,40)
	acf = {
		func = standing_wave
		amplitude = 2
		alpha = (0.12,0.06,0.06)
		velocity = 1.8
		beta = (0.8,0)
		grid = (10,10,10) : (2.5,5,5)
	}
	order = (10,10,10)
	algorithm = choi_recursive
#	order = (7,7,2)
#	algorithm = gauss_elimination
	output = surface,summary
}
velocity_potential_solver = linear {
#	wnmax = from (0,0) to (0,0.25) npoints (2,2)
	depth = 12
	domain = from (10,-12) to (10,3) npoints (1,128)
}
