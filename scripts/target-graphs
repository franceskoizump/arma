#!/usr/bin/gnuplot -persist

build_root=ARG1
cd build_root

files='\
white-noise \
elevation \
wave-period \
wave-height-x \
wave-length-x \
wave-height-y \
wave-length-y \
'

set terminal x11 size 1920/2, 1080
unset key
set multiplot layout 3, 3
do for [f in files] {
	set title f
	plot f with points, x with lines
}
